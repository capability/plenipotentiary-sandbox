{
  "name": "frontend-skeleton",
  "version": "0.0.1",
  "private": true,
  "type": "module",
  "packageManager": "pnpm@10.15.1",
  "engines": { "node": ">=22" },
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview --host 0.0.0.0",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "typecheck": "tsc -p tsconfig.json --noEmit",
    "test": "vitest run --reporter=verbose",
    "test:watch": "vitest"
  },
  "dependencies": {},
  "devDependencies": {
    "typescript": "5.9.2",
    "vite": "7.1.4",
    "eslint": "9.34.0",
    "@eslint/js": "9.34.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-import": "2.29.1",
    "globals": "15.8.0",
    "prettier": "3.3.3",
    "vitest": "3.2.4",
    "@vitest/coverage-v8": "3.2.4",
    "happy-dom": "18.0.1"
  },
  "pnpm": {
    "onlyBuiltDependencies": ["esbuild"]
  }
}
